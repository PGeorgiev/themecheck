{
    "name": "pgeorgiev/themecheck",
    "description": "Run Themecheck in WP_CLI",
    "type": "wp-cli-package",
    "homepage": "https://github.com/PGeorgiev/themecheck",
    "license": "MIT",
    "authors": [
        {
            "name": "Plamen Georgiev",
            "email": "hi@pgeorgiev.dev"
        }
    ],
    "autoload": {
        "files": [ "command.php" ]
    },
